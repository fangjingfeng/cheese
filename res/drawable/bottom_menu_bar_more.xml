<?xml version="1.0" encoding="utf-8"?>

<selector xmlns:android="http://schemas.android.com/apk/res/android">
	<item android:state_selected="true">
	    <bitmap android:src="@drawable/bottom_menu_bar_more_pressed" android:gravity="top" android:tileMode="disabled"/>
	</item>
    <item android:state_focused="true" >
        <bitmap android:src="@drawable/bottom_menu_bar_more_pressed" android:gravity="top" android:tileMode="disabled"/>
    </item>
    <item android:state_pressed="true">
        <bitmap android:src="@drawable/bottom_menu_bar_more_pressed" android:gravity="top" android:tileMode="disabled"/>
    </item>
    <item >
        <bitmap android:src="@drawable/menu_bottom_more_bt_normal" android:gravity="top" android:tileMode="disabled"/>
    </item>
    <item android:state_window_focused="false" >
        <bitmap android:src="@drawable/menu_bottom_more_bt_normal" android:gravity="top" android:tileMode="disabled"/>
    </item>
</selector>